<h1>订单列表</h1>

<table class="table">
  <tr>
    <th>Order_id</th>
    <th>Order_TotalPrice</th>
    <th>Order_created_at</th>
  </tr>
  <% @orders.each do |order| %>
  <tr>
    <td>
      <%= link_to order_path(order.token) do %>
        <%= order.id %>
      <% end %>
    </td>
    <td><%= order.total %></td>
    <td><%= order.created_at %></td>
  </tr>
  <% end %>
</table>
